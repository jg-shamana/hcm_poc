{
    "family": "hcm-poc-task-dev",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "AppContainer",
            "image": "AppImage",
            "essential": true,
                "logConfiguration": {
                "logDriver": "awslogs",
                    "options": {
                    "awslogs-group": "/ecs/hcm-poc-dev",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
    },
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "taskRoleArn": "arn:aws:iam::726854609615:role/hcm-ecs-dev-EcsTaskRole8DFA0181-45IPuvup6XXw",
    "executionRoleArn": "arn:aws:iam::726854609615:role/hcm-ecs-dev-EcsExecutionRoleFD93B7A2-pdEnZqrDSP7S",
    "cpu": "256",
    "memory": "512"
}
